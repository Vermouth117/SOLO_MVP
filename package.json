{
  "name": "flower-shop",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.11.16",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.34",
    "@types/react": "^18.2.8",
    "@types/react-dom": "^18.2.4",
    "@types/react-router-dom": "^5.3.3",
    "express": "^4.18.2",
    "knex": "^2.4.2",
    "pg": "^8.11.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.11.2",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "server": "node src/server.js",
    "dev": "nodemon src/server.js",
    "migrate-make": "knex migrate:make --knexfile src/db/knexfile.js",
    "migrate-latest": "knex migrate:latest --knexfile src/db/knexfile.js",
    "migrate-down": "knex migrate:down --knexfile src/db/knexfile.js",
    "migrate-back": "knex migrate:rollback --knexfile src/db/knexfile.js",
    "seed-make": "knex seed:make --knexfile src/db/knexfile.js",
    "seed-data": "knex seed:run --knexfile src/db/knexfile.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "nodemon": "^2.0.22"
  }
}
